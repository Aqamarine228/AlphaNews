{
    "name": "aqamarine/alphanews",
    "description": "Package used to easily and fast add News managing functionality to Laravel project",
    "type": "library",
    "keywords": [
        "alphanews",
        "alphadev",
        "news"
    ],
    "require": {
        "php": ">=8.1",
        "laravel/framework": "^9.0",
        "intervention/image": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-latest",
        "orchestra/testbench": "^7.6",
        "squizlabs/php_codesniffer": "^3.7",
        "friendsofphp/php-cs-fixer": "^3.9"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Aqamarine\\AlphaNews\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aqamarine\\AlphaNews\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Aqamarine228",
            "email": "MrFurgJK@gmail.com"
        }
    ],
    "prefer-stable": true,
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Aqamarine\\AlphaNews\\ServiceProvider"
            ]
        }
    }
}
